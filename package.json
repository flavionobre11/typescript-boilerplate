  },
  "homepage": "https://github.com/flavionobre11/cache-nodejs#readme",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/main.js",
    "env": "rm -f .env && cp .env.example .env",
    "start:dev": "ts-node-dev -r tsconfig-paths/register -r dotenv/config --respawn --transpile-only --clear --ignore-watch node_modules --no-notify src/main.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/express": "^4.17.6",
    "@types/jest": "~27.0.2",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "~27.3.1",
    "prettier": "^2.0.5",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.9.0",
    "ts-jest": "~27.0.7",
    "typescript": "~4.4.4"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1"
  }
}
